
<div class="topbar">
  <h2><a href="<%= _l "/#{@channel[:channel].gsub('#','')}" %>"><%= @channel[:channel] %></a></h2>
  <h3><a href="<%= _l "/#{@channel[:channel].gsub('#','')}/#{@current_date}" %>"><%= @date_title %></a> <span class="timezone">(<%= @tz.zone %>)</span></h3>
  <ul class="right">
    <li>
      <% if @yesterday  %>
        <a href="./<%= @yesterday %>" rel="prev">Prev</a>
      <% else %>
        <span class="disabled">Prev</span>
      <% end %>
    </li>
    <li>
      <% if @tomorrow %>
        <a href="./<%= @tomorrow %>" rel="next">Next</a>
      <% else %>
        <span class="disabled">Next</span>
      <% end %>
    </li>
  </ul>
  <div class="clear"></div>
</div>

<div class="logs">
  <div id="top" class="skip"><a href="#bottom">jump to bottom</a></div>

<table>
<% @logs.each do |line| %>
<%= format_line line %>
<% end %>
</table>

  <div id="bottom" class="skip"><a href="#top">jump to top</a></div>
</div>